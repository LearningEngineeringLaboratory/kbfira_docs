{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [
            ".",
            ".asset/vendor/general-ui",
            ".asset/vendor/kitbuild"
        ],
        "includePattern": "\\.js$",
        "exclude": [
            "node_modules",
            "docs"
        ]
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/dev/jsdoc_out",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "docdash": {
        "sectionOrder": [
            "Namespaces",
            "Classes",
            "Modules",
            "Externals",
            "Events",
            "Mixins",
            "Tutorials",
            "Interfaces"
        ],
        "search": true
    }
}